    "events": "^3.3.0",
    "nodemon": "^2.0.15",
    "parcel": "^2.3.2",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "querystring-es3": "^0.2.1",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "timers-browserify": "^2.0.12",
    "url": "^0.11.0",
    "util": "^0.12.4"
  },
  "dependencies": {
    "express": "^4.17.3",
